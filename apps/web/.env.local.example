# Copy this file to .env.local to configure the Next.js app for local development.

# Redis connection string used by the BullMQ render queue.
# If unset, the app falls back to an in-memory queue (single-process only).
#REDIS_URL=redis://127.0.0.1:6379

# Optional S3-compatible storage configuration. Leave blank to use the local storage/ directory.
#BRATGEN_S3_BUCKET=your-bucket
#BRATGEN_S3_REGION=us-east-1
#BRATGEN_S3_ENDPOINT=https://s3.amazonaws.com
#BRATGEN_S3_FORCE_PATH_STYLE=false

# Feature toggles
#BRATGEN_ENABLE_WHISPER=false
#BRATGEN_WHISPER_CACHE_TTL=86400
#BRATGEN_ENABLE_TELEMETRY=false

# Integrations
#SPOTIFY_CLIENT_ID=
#SPOTIFY_CLIENT_SECRET=
